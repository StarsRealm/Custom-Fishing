  "fishing_factory": {
    "type": "panel",
    "factory": {
      "name": "hud_title_text_factory",
      "control_ids": {
        "hud_title_text": "conditional_image@hud.conditional_image"
        
      }
    }
  },
  "little_bar_image": {
    "type": "stack_panel",
    "orientation": "vertical",
    "layer": 1,
    "propagate_alpha": true,
    "$atext": "$actionbar_text",
    "visible": "(not (($atext - 'customfishing:little_bar') = $atext))",
    "$offset": [ 0, 2 ],
    "offset": "$offset",
    "controls": [
      {
        "subtitle_frame": {
          "type": "panel",
          "size": [ "100%", "100%cm" ],
          "controls": [
            {
              "subtitle": {
                "type": "label",
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "text": "($atext - 'customfishing:little_bar:')",
                "layer": 31,
                "localize": false,
                "font_scale_factor": 2.2,
                "text_alignment": "center",
                "offset": [ 1, 19.5 ],
                "alpha": "@hud.anim_actionbar_text_alpha_out"
              }
            }
          ]
        }
      }
    ]
  },
  
  

  "little_bar_image_factory": {
    "type": "panel",
    "factory": {
      "name": "hud_actionbar_text_factory",
      "control_ids": {
        "hud_actionbar_text": "little_bar_image@hud.little_bar_image"
      }
    }
  },
  
  "hud_actionbar_text/actionbar_message": {
    "$atext": "$actionbar_text",
    "visible": "(($atext - 'customfishing:little_bar') = $atext)"
  },

  "root_panel": {
    "modifications": [
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [
          {
          "little_bar_image_factory@hud.little_bar_image_factory": {}
          },
          {
          "fishing_factory@fishing_factory": {}
          }
        ]
      }
    ]
  }
